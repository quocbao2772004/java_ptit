import java.util.*;
import java.io.*;
public class J01026 
{
    static boolean is_perfect_square_number(int n)
    {
        int x = (int) Math.sqrt(n);
        if(x * x == n) return true;
        return false;
    }
    public static void main(String[] args) throws FileNotFoundException
    {
//       Scanner sc = new Scanner(new File("J01016.txt"));
       Scanner sc = new Scanner(System.in);
       int test = sc.nextInt();
       while(test --> 0)
       {
           int n = sc.nextInt();
           if(is_perfect_square_number(n)) System.out.println("YES");
           else System.out.println("NO");
       }
    }
    
}
